#include <iostream>
using namespace std;
int fib(int n){
  //base case
  //first term
  if(n==1){
    return 0;
  }
  //second term
   if(n==2){
    return 1;
  }
  //recursive relation
  int ans=fib(n-1)+fib(n-2);
  //processing
  return ans;
}

int main() {
int n;
cin>>n;
int ans=fib(n);
cout<<ans;
  return 0;
}
