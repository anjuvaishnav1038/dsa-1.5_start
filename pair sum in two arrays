#include<iostream>
#include<vector>
using namespace std;
int main(){
 int sum=0;
  int n=9;
  vector<int>a{1,3,5,7};
  vector<int>b{2,4,6};
  for(int i=0;i<a.size();i++){
    for(int j=0;j<b.size();j++){
     sum=a[i]+b[j];
      if(n==sum){
    cout<<"("<<a[i]<<","<<b[j]<<")"<<" ";
  }
    }
    sum=0;
  }
  
return 0;
}
