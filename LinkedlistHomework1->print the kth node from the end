//--print the nth node from the end-GFG---//
//  https://youtu.be/c11mfsquJLk  //
class Solution{
public:
    int getNthFromLast(Node *head, int n)
    {
           // Your code here
           Node*first=head;
           Node*second=head;
           for(int i=1;i<n;i++){
               second=second->next;
         //AGR iterate krte krte second hi null ko point krne lgg jaye
              if(second==NULL)
              return-1;
           }
           while(second->next){
               first=first->next;
               second=second->next;
           }
        
           return   first->data;;
    }
};



