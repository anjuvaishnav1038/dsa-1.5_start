#include<iostream>
#include<queue>
using namespace std;
int main(){
   //creation
   queue<int>q;
   //insertion
   q.push(5);
   q.push(15);
   q.push(25);
   q.push(35);
   //size
   cout<<"size of the Queue--> "<<q.size()<<endl;
   //removal in queuue as it follows FIFO->first in first out
   q.pop();
   //now the number of elemnts in the queue
   cout<<"size of the  Queue--> "<<q.size()<<endl;
   //check wheather queue is empty or not
   if(q.empty()){
    cout<<"Queue is empty "<<endl;
   }
   else{
    cout<<"Queue is not empty "<<endl;
   }
   cout<<"Front element of the Queue is "<<q.front()<<endl;

   return 0;
}
