//find key in the string using recursion
#include <iostream>
using namespace std;
bool checkkey(string str,int i,int n,char key){
  //base case
  //poori array trverse key badd bhi ki ni mili
  if(i>=n){
    return false;
  }
  //ekk case solve krdo
  if(str[i]==key){
    return true;
  }
  //bakki recursion smbhal lega
  bool ans=checkkey( str,i+1,n,key);
  return ans;
}

int main() {
int i=0;
string str="googlebaba";
int n=str.length();
char key='r';
bool l=checkkey( str,i+1,n,key);
cout<<l;
  return 0;
}
